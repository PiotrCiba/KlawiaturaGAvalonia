<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:oxy="clr-namespace:OxyPlot.Avalonia;assembly=OxyPlot.Avalonia"
        xmlns:viewModels="clr-namespace:KlawiaturaGAvalonia.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="KlawiaturaGAvalonia.Views.FitnessGraph"
        x:DataType="viewModels:FitnessWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="Fitness Plot" Height="400" Width="600"
        Background="{Binding PrimaryColour}">
    <Grid>
        <oxy:PlotView Model="{Binding PlotModel}"/>
    </Grid>
</Window>
