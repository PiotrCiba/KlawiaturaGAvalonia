<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:KlawiaturaGAvalonia.ViewModels"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="150"
        x:Class="KlawiaturaGAvalonia.Views.SaveWindow"
        x:DataType="viewModels:MainWindowViewModel"
        Title="SaveWindow" Width="300" Height="100">
    
    <StackPanel VerticalAlignment="Center">
        <Button HorizontalAlignment="Center" Command="{Binding SaveCSV}" Content="Save to csv/txt"/>
        <Button HorizontalAlignment="Center" Command="{Binding CreateAhScript}" Content="Skrypt AH"/>
    </StackPanel>   
</Window>
